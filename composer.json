{
    "name": "cool-studio/globalpayments-magento2",
    "description": "Demonstrates integration with payment gateway from GlobalPayments, modified for PHP8.3",
    "require": {
        "php": "~8.1.0||~8.2.0||~8.3.0",
        "magento/framework": "103.0.*",
        "magento/module-sales": "103.0.*",
        "magento/module-checkout": "100.4.*",
        "magento/module-payment": "100.4.*",
        "magento/module-vault": "101.2.*",
        "globalpayments/php-sdk": "11.0.9",
        "psr/log": "^1 || ^2 || ^3"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "type": "magento2-module",
    "version": "2.1.6",
    "license": [
        "MIT"
    ],
    "autoload": {
        "files": [ "registration.php" ],
        "psr-4": {
            "GlobalPayments\\PaymentGateway\\": ""
        }
    }
}
